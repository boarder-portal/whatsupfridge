{
  "name": "whatsupfridge",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev-start": "npm-run-all --parallel client:build:watch server:build:watch server:start:watch",
    "prod-start": "NODE_ENV=production npm-run-all --sequential client:build server:build server:start",
    "client:build": "NODE_ENV=production webpack -c ./webpack/client.config.js",
    "client:build:watch": "webpack -w -c ./webpack/client.config.js",
    "server:build": "NODE_ENV=production webpack -c ./webpack/server.config.js",
    "server:build:watch": "webpack -w -c ./webpack/server.config.js",
    "server:start": "node ./build/server/main.js",
    "server:start:watch": "supervisor ./build/server/main.js",
    "analize-bundle": "ANALYZE_BUNDLE=1 npm run client:build",
    "analize-dev-bundle": "ANALYZE_BUNDLE=1 npm run client:build:watch",
    "generate-db": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' ts-node -r tsconfig-paths/register app/server/db/generateDB.ts",
    "typecheck": "tsc --noEmit",
    "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "eslint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "alias": {
    "server": "./app/server",
    "client": "./app/client",
    "common": "./app/common"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boarder-portal/whatsupfridge.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/boarder-portal/whatsupfridge/issues"
  },
  "homepage": "https://github.com/boarder-portal/whatsupfridge#readme",
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "@types/connect-redis": "0.0.14",
    "@types/redis": "^2.8.25",
    "bem-cn": "^3.0.1",
    "connect-redis": "^5.0.0",
    "dayjs": "^1.10.4",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "fast-deep-equal": "^3.1.3",
    "fs-extra": "^9.0.0",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react-router-dom": "^5.2.0",
    "redis": "^3.0.2",
    "regenerator-runtime": "^0.13.7",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^4.3.2",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-transform-react-jsx": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.0",
    "@types/express": "^4.17.2",
    "@types/express-session": "^1.17.0",
    "@types/fs-extra": "^9.0.0",
    "@types/loadable__component": "^5.13.3",
    "@types/loadable__server": "^5.12.5",
    "@types/lodash": "^4.14.170",
    "@types/morgan": "^1.7.37",
    "@types/react-router-dom": "^5.1.3",
    "@types/uuid": "^3.4.7",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "css-loader": "^5.2.6",
    "csstype": "^3.0.8",
    "eslint": "^6.8.0",
    "eslint-plugin-import-helpers": "1.0.2",
    "eslint-plugin-react": "^7.18.3",
    "eslint-plugin-react-hooks": "2.3.0",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.6.0",
